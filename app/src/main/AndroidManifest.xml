<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.vaibhav.AKGECchat">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_foreground"
        android:supportsRtl="true"
        android:theme="@style/Theme.ChatsApp"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".Screens.FeedBack"
            android:exported="true" />
        <activity
            android:name=".Screens.subjects"
            android:exported="true" />
        <activity
            android:name=".Screens.TimeTable"
            android:exported="true" />
        <activity
            android:name=".Screens.DashBoard2"
            android:exported="true"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".Activities.GroupChatActivity" />
        <activity
            android:name=".Activities.ChatActivity"
            android:theme="@style/Theme.ChatsApp.NoActionBar" />
        <activity android:name=".Activities.SetupProfileActivity" />
        <activity
            android:name=".Activities.OTPActivity"
            android:windowSoftInputMode="stateVisible" />
        <activity
            android:name=".Activities.PhoneNumberActivity"
            android:windowSoftInputMode="stateVisible">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activities.MainActivity"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".Screens.View_PDF_Files5" />
        <activity android:name=".Screens.DocsS" />
        <activity android:name=".Screens.View_PDF_Files4" />
        <activity android:name=".Screens.DocsA" />
        <activity android:name=".Screens.View_PDF_Files3" />
        <activity android:name=".Screens.DocsM2" />
        <activity android:name=".Screens.View_PDF_Files2" />
        <activity android:name=".Screens.DocsE" />
        <activity android:name=".Screens.View_PDF_Files1" />
        <activity android:name=".Screens.DocsP" />
        <activity android:name=".Screens.View_PDF_Files" />
        <activity android:name=".Screens.DocsM" />
        <activity
            android:name=".Screens.PlaySong"
            android:label="@string/title_activity_play_song"
            android:theme="@style/Theme.ChatsApp.NoActionBar" />
        <activity android:name=".Screens.iSangeet" />
        <activity android:name=".Screens.Ai" />
        <activity android:name=".Screens.SoftSkill" />
        <activity android:name=".Screens.Electrical" />
        <activity android:name=".Screens.Mechanics" />
        <activity android:name=".Screens.Physics" />
        <activity android:name=".Screens.Maths" />

        <activity android:name=".Screens.Section" />
        <activity android:name=".Screens.MyCollege" />
        <activity android:name=".Screens.Login" />
        <activity android:name=".Screens.Register" />
        <activity
            android:name=".Screens.Profile"
            tools:ignore="MissingClass" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <!--
 Set custom default icon. This is used when no icon is set for incoming notification messages.
     See README(https://goo.gl/l4GJaQ) for more.
        -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/ic_send" />
        <!--
 Set color used with incoming notification messages. This is used when no color is set for the incoming
             notification message. See README(https://goo.gl/6BKBk7) for more.
        -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/green" />
    </application>

</manifest>